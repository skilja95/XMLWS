<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
    targetNamespace="http://www.ftn.uns.ac.rs/presek" xmlns:gl="http://www.ftn.uns.ac.rs/xmlws"
    xmlns:pr="www.ftn.uns.ac.rs/presek">
    <xs:import namespace="http://www.ftn.uns.ac.rs/xmlws" schemaLocation="Utils.xsd"/>
    <xs:element name="presek">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="zaglavlje">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="broj_racuna" type="gl:TBrojRacuna"> </xs:element>
                            <xs:element name="datum_naloga" minOccurs="1" type="xs:date"/>
                            <xs:element name="broj_preseka" form="qualified">
                                <xs:simpleType>
                                    <xs:restriction base="xs:positiveInteger">
                                        <xs:totalDigits value="2"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="prethodno_stanje" type="gl:TIznos"> </xs:element>
                            <xs:element name="broj_promena_u_korist">
                                <xs:simpleType>
                                    <xs:restriction base="xs:positiveInteger">
                                        <xs:totalDigits value="6"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="broj_promena_na_teret" type="xs:positiveInteger"/>
                            <xs:element name="ukupno_u_korist" type="gl:TIznos"> </xs:element>
                            <xs:element name="ukupno_na_teret" type="gl:TIznos"> </xs:element>
                            <xs:element name="novo_stanje" type="gl:TIznos"> </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element maxOccurs="unbounded" name="stavka" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="duznik">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:minLength value="3"/>
                                        <xs:maxLength value="255"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="svrha_placanja">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:minLength value="3"/>
                                        <xs:maxLength value="255"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="poverilac">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:minLength value="3"/>
                                        <xs:maxLength value="255"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="datum_naloga" type="xs:date"/>
                            <xs:element name="datum_valute" type="xs:date"/>
                            <xs:element name="racun_duznika" type="gl:TRacun"> </xs:element>
                            <xs:element name="racun_poverioca" type="gl:TRacun"> </xs:element>
                            <xs:element name="iznos" type="gl:TIznos"> </xs:element>
                            <xs:element name="smer">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:length value="1"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
